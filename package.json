{
  "name": "oktaform-desktop",
  "version": "1.2.5",
  "private": true,
  "description": "Vue Material Kit",
  "author": "Creative Tim https://www.creative-tim.com/",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "NODE_ENV=DEV vue-cli-service serve & sleep 5 && NODE_ENV=DEV electron app.js",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run dev",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "start": "concurrently \"node server.js\" \"vue-cli-service serve\""
  },
  "main": "background.js",
  "dependencies": {
    "@hapi/hapi": "^19.1.1",
    "@mdi/font": "^5.3.45",
    "@okta/okta-vue": "^2.0.0",
    "axios": "^0.19.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "concurrently": "^5.2.0",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "electron": "^9.0.5",
    "epic-spinners": "^1.1.0",
    "express": "^4.17.1",
    "fix-path": "^3.0.0",
    "glob": "^7.1.6",
    "hcl-to-json": "^0.1.1",
    "jquery": "^3.4.1",
    "js-file-download": "^0.4.8",
    "lodash.orderby": "^4.6.0",
    "material-icons": "^0.3.1",
    "node-sass": "^4.13.0",
    "nouislider": "14.0.2",
    "pem": "^1.14.4",
    "v-datatable-light": "^0.8.0",
    "v-tooltip": "2.0.2",
    "vue": "2.6.10",
    "vue-carousel": "0.18.0",
    "vue-clickaway": "2.2.2",
    "vue-js-modal": "^1.3.31",
    "vue-lazyload": "1.3.1",
    "vue-loading-overlay": "^3.2.0",
    "vue-material": "1.0.0-beta-11",
    "vue-nav-tabs": "^0.5.7",
    "vue-router": "3.0.7",
    "vue-simple-spinner": "^1.2.8",
    "vue-spinner": "^1.0.3",
    "vue-spinner-component": "^1.0.5",
    "vuetify": "^2.3.5"
  },
  "protocols": {
    "name": "oktaform-protocol",
    "schemes": [
      "oktaform"
    ]
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.9.2",
    "@vue/cli-plugin-eslint": "3.9.2",
    "@vue/cli-service": "3.9.3",
    "@vue/eslint-config-prettier": "4.0.1",
    "electron": "^9.0.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-icon-builder": "^1.0.2",
    "sass-loader": "7.1.0",
    "vue-cli-plugin-electron-builder": "^2.0.0-rc.4",
    "vue-template-compiler": "2.6.10"
  }
}
